.container {
  /* background: linear-gradient(to right, #e6f6fe, #fff);   */
  background-color: #f8fbfe;
  padding-bottom: 25px;
}

.aui-palace {
  /* padding: 0.5rem 0; */
  position: relative;
  z-index: 99;
}

.aui-palace-one {
  padding: 15px 25px 18px 25px;
  color: #000;
  margin: 0 19px;
  margin-top: 23px;
  border-radius: 15px;
  background: #fff;
  box-shadow: 0px 1px 10px 2.03px rgba(35, 82, 184, 0.18);
}

.Nurse {
  display: flex;
  padding-bottom: 25px;
  border-bottom: dashed 1px #eee;
}

.head {
  border-radius: 100%;
  border: solid 4px #fcab51;
  width: 75px;
  height: 75px;
  position: relative;
}

.best {
  color: #fff;
  font-size: 14px;
  padding: 0 5px;
  border-radius: 4px;
  background-color: #fcab51;
  position: absolute;
  bottom: -10px;
  left: 3px;
}

.head_img {
  width: 75px;
  height: 75px;
  overflow: hidden;
  border-radius: 100%;
}

.head_img image {
  width: 100%;
  height: 100%;
}

.Star_mc {
  display: flex;
  margin-left: 20px;
  margin-top: 10px;
}

.Name {
  font-size: 22px;
  letter-spacing: 1px;
}

.Gender {
  margin-left: 20px;
}

.XB {
  background-color: #5dadfd;
  border-radius: 100px;
  text-align: center;
  line-height: 26px;
  color: #fff;
  font-size: 12px;
}

.FWSJ {
  margin-left: 10px;
  background-color: #73e746;
  border-radius: 100px;
  text-align: center;
  line-height: 26px;
  color: #fff;
  font-size: 12px;
}

.comment {
  margin-top: 10px;
  color: #696969;
  font-size: 16px;
  letter-spacing: 1px;
  font-weight: 600;
}



.aui-head-chang {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 13.5%;
  background-color: #188cff;
}