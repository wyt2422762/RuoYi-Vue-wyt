<!--pages/YYPH/YYPH.wxml-->
<i-toast id="toast" />

<view class="container aui-palace aui-palace-one2 aui-palace-two">
  <view wx:for="{{typeList}}" wx:for-index="index" wx:for-item="type"  wx:key="dictCode" class="aui-palace-grid" bindtap="goto" data-url="../JJPH_order/JJPH_order" data-typeid="{{type.dictValue}}" data-typename="{{type.dictLabel}}">
    <view class="aui-palace-grid-icon2">
      <image wx:if="{{index%2 === 0}}" src="../../img/icon_28.png"></image>
      <image wx:if="{{index%2 !== 0}}" src="../../img/icon_29.png"></image>
      {{type.dictLabel}}
    </view>
  </view>

</view>