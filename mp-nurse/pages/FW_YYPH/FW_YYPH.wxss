/* pages/JDGZ/JDGZ.wxss */
page {
  background-color: #f5f5f5;
}

.aui-palace {
  position: relative;
  z-index: 1;
  margin-top: 50px;
}

.aui-palace-one {
  position: relative;
  padding: 15px 25px 18px 25px;
  color: #131313;
  margin: 0 17px;
  margin-top: 23px;
  border-radius: 15px;
  background: #FFF;
  box-shadow: 0px 1px 10px 2.03px rgba(35, 82, 184, 0.18);
}

.name {
  font-size: 20px;
  font-weight: bold;
}

.list {
  border-bottom: dashed 1px #eee;
  padding-bottom: 20px;
}

.icon {
  width: 18px;
  height: 18px;
}

.icon image {
  width: 100%;
  height: 100%;
}

.Main {
  margin-top: 20px;
  display: flex;
  justify-content: space-between;
}

.contacts {
  display: flex;
  height: 18px;
  line-height: 18px;
}

.lxr {
  margin-left: 2px;
  height: 18px;
  line-height: 18px;
}

.refund {
  display: flex;
  justify-content: space-between;
}

.TK {
  background-color: transparent;
  margin: 15px 5px 0px 5px;
  height: 40px;
  line-height: 40px;
  text-align: center;
  width: 90%;
  border: solid 1px #ff8701;
  box-shadow: 0px 5px 10px 1.26px rgba(255, 135, 1, 0.4);
  color: #ff8701;
  letter-spacing: 1px;
  font-weight: normal;
  font-size: 18px;
  border-radius: 50px;
}

.Btn {
  margin: 15px 5px 0px 5px;
  height: 40px;
  line-height: 40px;
  text-align: center;
  width: 90%;
  border: solid 1px #188cff;
  background: #188cff;
  color: #fff;
  letter-spacing: 1px;
  font-weight: normal;
  font-size: 18px;
  border-radius: 50px;
  box-shadow: 0px 5px 10px 1.26px rgba(24, 140, 255, 0.4);
}

.ytk {
  position: absolute;
  top: 0;
  right: 0;
  font-size: 12px;
  background-color: #00bc31;
  border-top-right-radius: 15px;
  border-top-left-radius: 5px;
  border-bottom-right-radius: 5px;
  border-bottom-left-radius: 15px;
  color: #fff;
  padding: 4px 15px;
}

.swiper-box {
  display: block;
  height: calc(100% - 55px);
  width: 100%;
  margin-top: 30px;
  border: 1px solid red;
  overflow: hidden;
  padding-bottom: 10px;
}

.container{
  height: 100%;
}
