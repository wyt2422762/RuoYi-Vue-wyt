/* pages/login/login.wxss */

page {
  height: 100%;
  background-repeat: no-repeat;
  overflow: hidden;
  background-size: 100% 100%;
  -moz-background-size: 100% 100%;
}

.bj {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: -9999;
}

.bj image {
  width: 100%;
  height: 100%;
}

.bj_bottom {
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 320px;
z-index: -1;
}

.bj_bottom image {
  width: 100%;
  height: 100%;
}

.container {
  position: relative;
  height: 100%;
  display: flex;
  flex-direction: column;
  padding: 0;
  box-sizing: border-box;
  /* background-color: #f2f2f2;     */
}

/*登录图片*/
.login-icon {
  flex: none;
  margin: 0 auto;

}

.login-img {
  width: 180rpx;
  height: 180rpx;
  margin-top: 100rpx;
}

.login-img2 {
  width: 173rpx;
  height: 39rpx;
  margin-top: 30rpx;
}


/*表单内容*/
.login-from {
  margin-top: 60px;
  flex: auto;
  height: 100%;
}

.weui-cell_input {
  margin-top: 15px;
}

/* 输入框 */
.myno {
  width: 75%;
  height: 80rpx;
  margin: 0 auto;
  border-bottom: 1px #fff solid;

}

.mypwd {
  width: 75%;
  height: 80rpx;
  margin: 0 auto;
  border-bottom: 1px #fff solid;
  margin-top: 20rpx;
}

/*按钮*/
.loginBtnView {
  margin-top: 0px;
  margin-bottom: 0px;
  padding-bottom: 0px;
}

.loginBtn {
width: 100%;
  height: 90rpx;
  font-size: 16px;
  letter-spacing: 2px;
  font-weight: bold;
  line-height: 60rpx;
  margin-top: 30px;
  color: #fff;
  background: #fe96c4;
  border-radius: 100px;
}

.weui-cell__bd input {
  color: #fff;
  margin-left: 10px;
}

.place-holder {
  /* WebKit browsers */
  color: #fff;

}

.nameImage,
.keyImage {
